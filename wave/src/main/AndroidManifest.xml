<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.magicsoft.wave">


    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".App"
        android:allowBackup="true" android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true" android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.OtherActivity"/>
        <activity android:name=".activity.SeekActivity"/>
        <activity android:name=".activity.ClipActivity"/>
        <activity android:name=".activity.PatternActivity"/>

        <activity android:name=".activity.ScrollViewActivity"/>
        <activity android:name=".activity.SeekTextActivity"/>
        <activity android:name=".activity.StatusActivity"/>
        <activity android:name=".activity.ClipDataActivity"/>

        <activity android:name=".activity.TouchActivity"/>
        <activity android:name=".activity.ExaminActivity"/>
        <activity android:name=".activity.PhotoActivity"/>
        <activity android:name=".activity.ShadowActivity"/>
        <activity android:name=".activity.VagueActivity"/>
        <activity android:name=".activity.VagueActivity2"/>

        <activity android:name=".activity.VagueActivity3"/>
    </application>

</manifest>